{
    "name": "OpenVPN Client",
    "version": "0.2.3",
    "slug": "openvpn",
    "description": "OpenVPN Client",
    "url": "https://github.com/Beduir/hassio-custom-addons",
    "startup": "application",
    "boot": "auto",
    "options": {
        "ovpn_file": ""
    },
    "schema": {
        "ovpn_file": "str"
    },
    "arch": [
      "aarch64",
      "amd64",
      "armhf",
      "i386"
    ],      
    "map": [ "config" ],
    "host_network": true,
    "devices": [ "/dev/net/tun:/dev/net/tun:rw" ],
    "privileged": [ "NET_ADMIN" ]
}
